{
    "version": 2,
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "/api/index.js"
        },
        {
            "src": "/(.*)",
            "dest": "/frontend/$1",
            "check": true
        },
        {
            "src": "/(.*)",
            "dest": "/frontend/index.html"
        }
    ]
}